@using BankingSDK.Common.Models.Data
@model TestWebApp.Models.AccountListViewModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>title</h2>
<ul>
    @foreach (Account account in Model.Accounts)
    {
        <li>@account.Iban / @account.Description / @account.Currency - <a href="@Url.Action("ListTransactions", "BeBelfius")?accountId=@account.Id">Transactions</a> </li>
    }
</ul>