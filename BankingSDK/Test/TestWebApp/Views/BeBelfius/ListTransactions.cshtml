@using BankingSDK.Common.Models.Data
@model TestWebApp.Models.TransactionListViewModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>title</h2>
<ul>
    @foreach (Transaction transaction in Model.Transactions)
    {
        <li>
            @transaction.Amount@transaction.Currency 
            to/from @transaction.CounterpartReference (@transaction.CounterpartName) 
            at Value date : @transaction.ValueDate - Exec date : @transaction.ExecutionDate<br/>
            <i>@transaction.Description</i>
        </li>
    }
</ul>